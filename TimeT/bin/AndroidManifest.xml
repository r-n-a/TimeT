<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.timet"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/Itime"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Holo" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ViewEm"
            android:label="@string/title_activity_view_em" >
        </activity>
        <activity
            android:name=".EmpActivity"
            android:label="@string/title_activity_emp" >
        </activity>
        <activity
            android:name=".AddActivity"
            android:label="@string/title_activity_add" >
        </activity>
        <activity
            android:name=".AddEmActivity"
            android:label="@string/title_activity_add_em" >
        </activity>
        <activity
            android:name=".ListTimeActivity"
            android:label="@string/title_activity_list_time" >
        </activity>
    </application>

</manifest>
